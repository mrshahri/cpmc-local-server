      <xs:element name='ComponentOperation' type='ComponentOperationType' minOccurs='0' maxOccurs='unbounded'>
        <xs:annotation>
          <xs:documentation>
            The Operation of each component
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name='DeviceOperationType'>
    <xs:annotation>
      <xs:documentation>
        Operation related to a single device
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name='Actions' type='ActionsType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            A collection of Actions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='Jobs' type='JobsType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            A collection of Jobs
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name='name' type='NameType' use='required'/>
    <xs:attribute name='uuid' type='UuidType' use='required'/>
